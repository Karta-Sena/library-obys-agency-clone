    "extract:book-details": "node scripts/extract-book-details.mjs",
    "download:assets": "python scripts/download-assets.py",
    "normalize:assets": "node scripts/normalize-obys-assets.mjs"
  },
  "dependencies": {
    "gsap": "^3.14.2",
    "next": "14.2.5",
